{
  "name": "node-tagged-cache",
  "description": "In-memory, performance-targeted caching module with ttl and tagging support.",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/xobotyi/node-tagged-cache.git"
  },
  "bugs": {
    "url": "https://github.com/xobotyi/node-tagged-cache/issues"
  },
  "homepage": "https://github.com/xobotyi/node-tagged-cache",
  "author": "Anton Zinovyev",
  "license": "MIT",
  "tags": [
    "node",
    "tagged",
    "cache",
    "inmemory-cache",
    "memcached",
    "inmemory",
    "memory",
    "internal",
    "local"
  ],
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "format:fix": "prettier --write src/**/*.ts benchmarks/**/*.js",
    "format:list": "prettier -l src/**/*.ts benchmarks/**/*.js",
    "preversion": "npm run test && npm run build && npm run format:fix",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run test && npm run build",
    "push-codacy-coverage": "cat ./coverage/lcov.info | codacy-coverage -p ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:fix && npm run build && git add .",
      "post-commit": "git update-index -g"
    }
  },
  "dependencies": {
    "kind-of": "^6.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "cache-base": "^4.0.0",
    "clone": "^2.1.2",
    "clone-deep": "^4.0.1",
    "husky": "^4.2.2",
    "jest": "^23.6.0",
    "lodash.clonedeep": "^4.5.0",
    "node-cache": "^5.1.0",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "webSocketDebuggerUrl": "ws://127.0.0.1:9229/d270c0dd-6de4-4ced-ae66-7469a15f1d97"
}
